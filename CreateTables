CREATE TABLE Classmates (
    id INT PRIMARY KEY,
    name VARCHAR(50),
    age INT
);

CREATE TABLE Coursemates (
    id INT PRIMARY KEY,
    name VARCHAR(50),
    course INT
);

CREATE TABLE Disciplines (
    id INT PRIMARY KEY,
    title VARCHAR(50),
    teacher VARCHAR(50)
);

CREATE TABLE Books (
    id INT PRIMARY KEY,
    title VARCHAR(50),
    author VARCHAR(50)
);

CREATE TABLE Movies (
    id INT PRIMARY KEY,
    title VARCHAR(50),
    director VARCHAR(50)
);

CREATE TABLE Cities (
    id INT PRIMARY KEY,
    name VARCHAR(50),
    country VARCHAR(50)
);
 